{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/essential", "@vue/prettier"],
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "spaced-comment": "error", // 注释前面必须有空格
    "vue/attributes-order": "error", //标签属性必须按规则排序
    "vue/order-in-components": "error" //组件的属性必须为一定的顺序
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
